<?xml version="1.0" encoding="utf-8"?>
<resources  xmlns:tools="http://schemas.android.com/tools">




    <attr name="fontPath" format="string"/>

    <declare-styleable name="CardContainer">
        <attr name="android:gravity"/>
        <attr name="orientation"></attr>
    </declare-styleable>





    <declare-styleable name="superlistview">
        <attr name="superlv__listSelector" format="reference"/>
        <attr name="superlv__empty" format="reference"/>
        <attr name="superlv__moreProgress" format="reference"/>
        <attr name="superlv__progress" format="reference"/>
        <attr name="superlv__listDivider" format="reference"/>
        <attr name="superlv__listDividerHeight" format="dimension"/>
        <attr name="superlv__listClipToPadding" format="boolean"/>
        <attr name="superlv__listPadding" format="dimension"/>
        <attr name="superlv__listPaddingTop" format="dimension"/>
        <attr name="superlv__listPaddingBottom" format="dimension"/>
        <attr name="superlv__listPaddingLeft" format="dimension"/>
        <attr name="superlv__listPaddingRight" format="dimension"/>
        <attr name="superlv__scrollbarStyle">
            <flag name="insideOverlay" value="0x0"/>
            <flag name="insideInset" value="0x01000000"/>
            <flag name="outsideOverlay" value="0x02000000"/>
            <flag name="outsideInset" value="0x03000000"/>
        </attr>

        <!-- Layout to build a superList. Default values are  @layout/view_progress_listview for a list and
         @layout/view_progress_gridview for a grid-->
        <attr name="superlv_mainLayoutID" format="reference"/>
    </declare-styleable>

    <declare-styleable name="supergridview">
        <attr name="supergv__columns" format="integer"/>
        <attr name="supergv__verticalSpacing" format="dimension"/>
        <attr name="supergv__horizontalSpacing" format="dimension"/>
    </declare-styleable>




















    <declare-styleable name="CustomAttributes">
        <!--        indicate if the slider must show number indicator -->
        <attr name="showNumberIndicator" format="boolean" />
        <!--        in progress view indicate max value of progress -->

        <!--        in progress view indicate min value of progress -->
        <attr name="min" format="integer" />
        <!--        in progress view indicate value of progress -->
        <attr name="value" format="integer" />
        <!--        in progress view indicate value of progress -->

        <!--        in switche's view indicate state of view -->
        <attr name="check" format="boolean" />
        <!--        in float button indicate icon resource -->
        <attr name="iconFloat" format="integer" />
        <!--        in float button indicate if must start with animation -->
        <attr name="animate" format="boolean" />
        <!--        Speed of ripple animation -->
        <attr name="rippleSpeed" format="float" />
        <!--        in float button indicate if must start with animation -->

    </declare-styleable>








    <declare-styleable name="ProgressLayout">
        <attr name="progressLayoutProgressBackground" format="color"/>
        <attr name="progressLayoutProgress" format="boolean"/>
    </declare-styleable>




    <!--<declare-styleable name="match">-->
        <!--<attr name="matchViewText" format="string" />-->
        <!--<attr name="matchViewTextColor" format="color" />-->
        <!--<attr name="matchViewTextSize" format="dimension" />-->
    <!--</declare-styleable>-->

    <declare-styleable name="match">
        <attr name="text"/>
        <attr name="textColor"/>
        <attr name="textSize"/>
    </declare-styleable>






    <declare-styleable name="NumberProgressBar">
        <attr name="progress_num" format="integer" />
        <attr name="number_progressbar_max" format="integer" />
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />

        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />

        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />

        <attr name="progress_text_offset" format="dimension" />

        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NumberProgressBarStyle">
        <attr name="numberProgressBarStyle" format="reference" />
    </declare-styleable>





    <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize"></attr>
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="dimension" />
    </declare-styleable>
</resources>