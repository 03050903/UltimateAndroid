<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
        >


    <com.fss.Common.uiModule.enhanceListView.EnhancedListView android:layout_width="fill_parent"
                                                              android:id="@+id/landingEnhanceListView"
                                                              android:layout_height="wrap_content">

    </com.fss.Common.uiModule.enhanceListView.EnhancedListView>
    <android.support.v4.view.ViewPager android:layout_width="match_parent" android:layout_height="80dp"
                                       android:visibility="visible" android:id="@+id/landingMallViewpager"
            ></android.support.v4.view.ViewPager>
    <com.fss.Common.uiModule.viewpagerindicator.CirclePageIndicator android:layout_width="fill_parent"
                                                                    android:layout_below="@id/landingMallViewpager"
                                                                    android:id="@+id/landingMallViewPagerIndicator"
                                                                    android:layout_height="wrap_content"></com.fss.Common.uiModule.viewpagerindicator.CirclePageIndicator>


</LinearLayout>

